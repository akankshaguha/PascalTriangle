package com.bitwise.collection

object sumofnumbers {
  def main(args:Array[String]){
    var array = new Array[Integer](4)
    array(0)=1
    array(1)=2
    array(2)=3
    array(3)=4
    
    for(x <- array){
      print(x)
    }
    var total:Int =0;
    for(x <-0 to (array.length-1)){
      total+=array(x);
    }
    print(total)
  }
}