object Hello {
  
  
  
   def fact(n: Int, r: Int=1): Int = if (n==0) r else fact(n-1, n*r)

  def pascal(c: Int, r: Int): Int = {
    fact(c) / (fact(r) * fact(c-r))
  }

  def main(a: Array[String]) { 
    
    for(r<-0 to 10){
      for(c<-0 to r)
    
    println(pascal(r, c))
  }
    println()
  }
}